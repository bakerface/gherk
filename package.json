{
  "name": "gherk",
  "version": "2.0.0",
  "description": "A minimalistic parser for the Gherkin syntax",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "author": "Chris Baker <mail.chris.baker@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bakerface/gherk.git"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "gherkin",
    "feature",
    "scenario",
    "given",
    "when",
    "then"
  ],
  "scripts": {
